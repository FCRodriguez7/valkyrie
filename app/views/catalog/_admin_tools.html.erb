<div class="panel panel-default admin-tools">
  <div class="panel-heading">
    Admin Tools
  </div>

  <div class="panel-body">
    <ul class="nav">
      <li>
        <%= link_to "Edit", polymorphic_path([:edit, document.resource]) %>
